<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="dal.Customer" table="customer">

<id name="custid" column="custid"  />
<property name="custname" column="custname" length="10"/>

<property name="vid" column="vid"  insert="false" />

</class>
</hibernate-mapping>